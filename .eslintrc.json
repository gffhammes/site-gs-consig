{
  "extends": "next/core-web-vitals",
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "tsconfig.json"
  },
  "plugins": ["@typescript-eslint", "unused-imports"],
  "rules": {
    "no-console": "warn",
    "no-unused-vars": "warn",
    "unused-imports/no-unused-imports": "warn",
    "@typescript-eslint/no-unused-vars": "warn"
  }
}